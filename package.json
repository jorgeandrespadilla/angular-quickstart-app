{
  "name": "angular-quickstart-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:development",
    "start:development": "cross-env NODE_ENV=development ng serve",
    "start:production": "cross-env NODE_ENV=production ng serve",
    "build": "cross-env NODE_ENV=production ng build",
    "test": "cross-env NODE_ENV=development ng test",
    "test:watch": "cross-env NODE_ENV=development ng test --watch",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "storybook": "ng run angular-quickstart-app:storybook",
    "storybook:build": "ng run angular-quickstart-app:build-storybook",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:serve": "compodoc -s",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.10.3",
    "@analogjs/vite-plugin-angular": "^1.9.0",
    "@analogjs/vitest-angular": "^1.9.0",
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@chromatic-com/storybook": "^3.2.3",
    "@compodoc/compodoc": "^1.1.26",
    "@ngx-env/builder": "^19.0.4",
    "@playwright/test": "1.49.1",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/angular": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/angular": "^17.3.5",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/user-event": "^14.5.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "angular-eslint": "19.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.11.1",
    "jsdom": "^22.0.0",
    "playwright-ng-schematics": "^1.2.2",
    "storybook": "^8.4.7",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.18.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^2.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
